<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deliveryapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:name=".DeliveryApp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
 
        <activity
            android:name=".CategoriesActivity"
            android:exported="false" />

        <activity android:name=".OderList"></activity>
        <activity android:name=".AdminSection" />
        <activity android:name=".RegisterDeliverer" />
        <activity android:name=".ItemsManager" />
 
        <activity
            android:name=".SignUpActivity"
            android:exported="false" />
        <activity android:name=".SignIn" />
        <activity android:name=".DashBoard" />
        <activity android:name=".WalkThrough" />




        <activity android:name=".Splash">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>